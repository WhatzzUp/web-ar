{
  "name": "emotion-detection",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "test": "jest",
    "clone:openCV": "git clone https://github.com/opencv/opencv.git",
    "build:openCV:mac": "npm run clone:openCV && docker run --rm --workdir /code -v \"$PWD\":/code \"trzeci/emscripten:latest\" python ./opencv/platforms/js/build_js.py build",
    "build:openCV:windows": "npm run clone:openCV && docker run --rm --workdir /code -v \"$(get-location):/code\" \"trzeci/emscripten:latest\" python ./opencv/platforms/js/build_js.py build "
  },
